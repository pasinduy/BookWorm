<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.bookworm.controller.UserNavigationPanelController">
    <children>
        <AnchorPane fx:id="dashboardNav" prefHeight="700.0" prefWidth="250.0" stylesheets="@../style/navPanel.css">
            <children>
                <Button graphicTextGap="10.0" layoutX="35.0" layoutY="160.0" mnemonicParsing="false" onAction="#dashboard" prefHeight="0.0" prefWidth="200.0" stylesheets="@../style/navPanel.css" text="Dashboard">
                    <graphic>
                        <ImageView fitHeight="49.0" fitWidth="66.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../assets/Dashboard.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button graphicTextGap="25.0" layoutX="35.0" layoutY="245.0" mnemonicParsing="false" onAction="#history" prefHeight="0.0" prefWidth="200.0" stylesheets="@../style/navPanel.css" text="Books">
                    <graphic>
                        <ImageView fitHeight="57.0" fitWidth="57.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../assets/read.gif" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button graphicTextGap="20.0" layoutX="35.0" layoutY="330.0" mnemonicParsing="false" onAction="#transaction" prefHeight="0.0" prefWidth="200.0" stylesheets="@../style/navPanel.css" text="Reservation">
                    <graphic>
                        <ImageView fitHeight="49.0" fitWidth="66.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../assets/reading-book.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button graphicTextGap="30.0" layoutX="35.0" layoutY="500.0" mnemonicParsing="false" onAction="#settings" prefHeight="0.0" prefWidth="200.0" stylesheets="@../style/navPanel.css" text="Settings">
                    <graphic>
                        <ImageView fitHeight="51.0" fitWidth="52.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../assets/computer.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button graphicTextGap="30.0" layoutX="35.0" layoutY="585.0" mnemonicParsing="false" onAction="#LogOut" prefHeight="0.0" prefWidth="200.0" stylesheets="@../style/navPanel.css" text="Log Out">
                    <graphic>
                        <ImageView fitHeight="51.0" fitWidth="52.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../assets/log-out.gif" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children></AnchorPane>
        <AnchorPane fx:id="SecRoot" layoutX="250.0" prefHeight="700.0" prefWidth="950.0" stylesheets="@../style/navPanel.css" />
    </children>
</AnchorPane>
